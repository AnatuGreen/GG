{
  "expo": {
    "name": "GistGate",
    "slug": "GistGate",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/iconnew.png",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconnew.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anatugreen.GistGate"
    },
    "web": {
      "favicon": "./assets/iconnew.png"
    }
  }
}
